{
    "manifest_version": 2,
    "name": "Editor++",
    "version": "1.0.0",

    "description": "Editor++ is a mod for bonk2.io that improves the built-in editor.",

    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "96": "icons/96.png",
        "128": "icons/128.png"
    },

    "permissions": ["*://bonk2.io/beta/*", "webRequest", "webRequestBlocking"],

    "web_accessible_resources": ["content.bundle.js"],

    "background": {
        "scripts": ["./background.bundle.js"]
    }
}
